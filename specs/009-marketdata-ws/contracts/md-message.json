{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Md-message",
  "type": "object",
  "required": ["type","instrumentId","marketData"],
  "properties": {
    "type": { "const": "Md" },
    "instrumentId": {
      "type": "object",
      "required": ["marketId","symbol"],
      "properties": {
        "marketId": { "type": "string" },
        "symbol": { "type": "string" }
      }
    },
    "marketData": { "type": "object" },
    "sequenceId": { "oneOf": [{"type":"integer"},{"type":"string"}] },
    "timestamp": { "oneOf": [{"type":"string"},{"type":"integer"}] }
  },
  "additionalProperties": true
}
