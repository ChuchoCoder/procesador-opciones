{
    "manifest_version": 3,
    "name": "Procesador de opciones",
    "version": "1.0.2",
    "description": "Procesador de operaciones financieras con persistencia en almacenamiento del navegador",
    "permissions": [
        "storage",
        "alarms"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background": {
        "service_worker": "background/instruments-sync.js",
        "type": "module"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "Procesador de opciones"
    }
}
